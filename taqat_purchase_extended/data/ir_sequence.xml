<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="seq_variant_purchase_order" model="ir.sequence">
            <field name="name">Variant Purchase Order</field>
            <field name="code">variant.purchase.order</field>
            <field name="prefix">V</field>
            <field name="padding">0</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="approved_seq_purchase_order" model="ir.sequence">
            <field name="name">Approved Purchase Order</field>
            <field name="code">approved.purchase.order</field>
            <field name="prefix">PO</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
